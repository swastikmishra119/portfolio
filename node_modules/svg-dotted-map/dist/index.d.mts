interface Region {
    lat: {
        min: number;
        max: number;
    };
    lng: {
        min: number;
        max: number;
    };
}
type Marker<MarkerData = void> = {
    lat: number;
    lng: number;
    size?: number;
} & MarkerData;
interface CreateMapOptions {
    height: number;
    width: number;
    radius?: number;
    countries?: string[];
    mapSamples?: number;
    region?: Region;
}
type Point = {
    x: number;
    y: number;
};

declare const createMap: ({ height, width, countries, region, radius, mapSamples, }: CreateMapOptions) => {
    points: Point[];
    addMarkers: <MarkerData>(markers: Array<Marker<MarkerData>>) => ({
        x: number;
        y: number;
    } & Omit<Marker<MarkerData>, "lat" | "lng">)[];
};

export { createMap };
